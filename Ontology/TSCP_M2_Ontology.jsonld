{
  "@context": {
    "tscp": "https://github.com/Echopraxium/tscp-framework/ontology/",
    "m3": "https://github.com/Echopraxium/tscp-framework/ontology/TSCP_M3_Ontology.jsonld",
    "m2": "https://github.com/Echopraxium/tscp-framework/ontology/m2#",
    "rdfs": "http://www.w3.org/2000/01/rdf-schema#",
    "sig": "tscp:tensorSignature",
    "derivedFrom": { "@id": "tscp:derivedFrom", "@type": "@id" },
    "requires": { "@id": "tscp:requires", "@type": "@id" }
  },
  "@id": "tscp:TSCP_M2_Ontology.jsonld",
  "@graph": [
    { "@id": "m2:Space", "sig": "\\mathcal{V} \\subseteq \\mathbb{R}^n" },
    { "@id": "m2:Boundary", "sig": "\\partial \\mathcal{V}", "derivedFrom": "m2:Space" },
    { "@id": "m2:Environment", "sig": "\\mathcal{V} \\setminus \\mathcal{V}_{ag}" },
    { "@id": "m2:System", "sig": "\\bigoplus \\mathcal{V}_i" },
    { "@id": "m2:Agent", "sig": "\\mathcal{A}: \\mathcal{V} \\to \\mathcal{V}" },
    { "@id": "m2:Identity", "sig": "\\ker(\\mathbf{T} - \\mathbf{I})" },
    { "@id": "m2:Facet", "sig": "\\mathbf{P}_{\\phi} \\vec{v}", "derivedFrom": "m2:Identity" },
    { "@id": "m2:Attribute", "sig": "\\vec{v} \\in \\mathcal{V}" },
    { "@id": "m2:Resource", "sig": "\\| \\vec{v}_{rs} \\|" },
    { "@id": "m2:Energy", "sig": "E = \\int \\vec{F} \\cdot d\\vec{s}", "derivedFrom": "m2:Resource" },
    { "@id": "m2:Gradient", "sig": "\\nabla \\Phi" },
    { "@id": "m2:Threshold", "sig": "H(\\|\\nabla \\Phi\\| - \\theta)", "requires": "m2:Gradient" },
    { "@id": "m2:Role", "sig": "\\mathbf{R} \\in \\mathbb{R}^{m \\times n}" },
    { "@id": "m2:Behavior", "sig": "\\{ \\mathbf{M}_k \\}" },
    { "@id": "m2:Rule", "sig": "\\sigma(\\mathbf{W}\\vec{v} + b)", "derivedFrom": "m2:Constraint" },
    { "@id": "m2:Protocol", "sig": "\\sigma \\circ \\mathcal{B}", "requires": "m2:Rule" },
    { "@id": "m2:Signal", "sig": "\\vec{s}(t)" },
    { "@id": "m2:Representation", "sig": "\\mathbf{Q}: \\mathcal{V}_c \\to \\mathcal{V}_a" },
    { "@id": "m2:Language", "sig": "\\mathcal{L} = \\{ \\vec{s} \\mid G(\\vec{s}) \\}", "requires": "m2:Representation" },
    { "@id": "m2:Mode", "sig": "\\lambda \\in \\text{spec}(\\mathbf{T})" },
    { "@id": "m2:Objective", "sig": "\\text{arg min } \\mathcal{L}(\\vec{v}, \\vec{v}^*)" },
    { "@id": "m2:Constraint", "sig": "\\mathbf{C}\\vec{v} = \\vec{d}" },
    { "@id": "m2:Interaction", "sig": "\\vec{a} \\otimes \\vec{b}" },
    { "@id": "m2:Transformation", "sig": "\\mathbf{T} \\cdot \\vec{v}", "requires": "m2:Energy" },
    { "@id": "m2:Transduction", "sig": "\\mathcal{T}: \\vec{s}_{in} \\otimes E \\to \\vec{s}_{out}" },
    { "@id": "m2:Event", "sig": "\\delta(t - t_0)" },
    { "@id": "m2:Attractor", "sig": "\\vec{v}_{\\infty}" },
    { "@id": "m2:Synergy_Pos", "sig": "\\langle \\vec{a}, \\vec{b} \\rangle > 0" },
    { "@id": "m2:Synergy_Neg", "sig": "\\langle \\vec{a}, \\vec{b} \\rangle < 0" },
    { "@id": "m2:Temporality", "sig": "t \\in \\mathbb{R}^+" }
  ]
}